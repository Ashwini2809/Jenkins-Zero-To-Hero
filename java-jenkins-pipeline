pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout your code repository (assuming it's a Git repository)
                git 'https://github.com/ashwini2809/your-java-project.git'
            }
        }

        stage('Build') {
            steps {
                // Build your Java application (replace 'javac' and 'java' with your build tools)
                sh 'javac -cp . src/HelloWorld.java'
            }
        }

        stage('Run') {
            steps {
                // Run your Java application
                sh 'java -cp . src/HelloWorld'
            }
        }
    }
}
